{
  "name": "vue-3-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,vue,json}": [
      "prettier --write -u"
    ]
  },
  "dependencies": {
    "@vueuse/core": "^13.0.0",
    "ant-design-vue": "^4.2.6",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.2",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-sort-json": "^3.1.0",
    "sass": "^1.68.0",
    "vite": "^4.4.5",
    "vite-plugin-eslint": "^1.8.1"
  }
}
